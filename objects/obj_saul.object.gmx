        <arguments>
          <argument>
            <kind>1</kind>
            <string>//////////////////////////
// Core-functionalities
// Do not touch these
// Scroll all the way down for changeable features
/////////////////////////

script_execute(scr_enemycorestep);

////////////////////////////
//Add additional step features here
////////////////////////////

shootTimer -= delta_time / 1000000;
moveTimer  -= delta_time / 1000000;

script_execute(scr_resyncenemies_david);

if (global.bosstwo &gt; 0 &amp;&amp; global.bossthree &lt; 1 &amp;&amp; global.bossone &gt; 0)
{
    if(hpIndex == 3)
    {
        hp = global.bossthree;
        hpIndex = 1;
    }
    global.bosstwo = hp;
}

if (global.bosstwo &lt; 1 &amp;&amp; global.bossthree &lt; 1 &amp;&amp; global.bossone &gt; 0)
{
    if(hpIndex == 2)
    {
        hp = global.bossone;
        hpIndex = 1;
    }
    global.bossone = hp;
}

if (shootTimer &lt;= 0)
{
    // ======== SHOOTING ====================
    //Reset timer
    if (shotCount &lt; shotsBeforeRest)
    {
        shootBeat = global.quarterSec;
        shootTimer = shootBeat;
        shotCount += 1;
    }
    else
    {
        shotCount = 0;
        chosenAttack = -1;
        attackDelay = 0;
        gatlingShot = 0;
        shootBeat = global.wholeSec;
        shootTimer = shootBeat;
    }
    
    //Perform shooting
    if (chosenAttack = 0)
    {
        
        
        //Must shoot 2 regulars first so cancel top if so
        if (normalAttack &lt; 2)
        {
            chosenAttack = 3;
            normalAttack += 1;
            if (random_range(0, 100) &lt; 80)
            {
                audio_play_sound_on (global.voiceEmitter, snd_saulfreeze, false, 10); 
            }
            else
            {
                audio_play_sound_on (global.voiceEmitter, snd_saulyah, false, 10); 
            }
           
            sprite_index = spr_policechiefshoot;
            image_index = 0;
            image_speed = 1;
            script_execute(scr_fliptoplayer);
            
            if (instance_exists(obj_chizuru))
            {
                aimCenter = point_direction(x, y, obj_chizuru.x, obj_chizuru.y);
            }
            aimDirection = 0;
            
        }
        else
        {
            normalAttack = 0;
            if (random_range(0, 100) &gt; 50 )
            {
                audio_play_sound_on (global.effectsEmitter, snd_ricebullet, false, 10); 
                audio_play_sound_on (global.voiceEmitter, snd_saulvolcano, false, 10);
                bomb = instance_create(x, y,  obj_grenadespread_sonii);
                bomb.speed = 7;
                if (instance_exists(obj_chizuru))
                {
                    bomb.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + random_range(-40, 40);
                }
                chosenAttack = 2;
                shotCount = 0;
                shotsBeforeRest = 12;
            }
            else
            {
                audio_play_sound_on (global.effectsEmitter, snd_pineapplethrow, false, 10); 
                audio_play_sound_on (global.voiceEmitter, snd_saulcrash, false, 10);
                for (i = 0; i &lt; 4; i++)
                {
                    grenade[i] = instance_create(x, y, obj_saulgrenade);
                    if (instance_exists(obj_chizuru))
                    {
                        grenade[i].direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + random_range(-60, 60);
                    }
                    grenade[i].speed = 10 + random_range(-5, 20);
                }
                chosenAttack = 1;
                shotCount = 0;
                shotsBeforeRest = 8;
            }
            
        }
    }
    
    //Gatling Attack
    if (chosenAttack == 3)
    {
        if(attackDelay &lt; 1)
        {
            attackDelay += 1;
        }
        else
        {
            if (gatlingShot == 0)
            {
                shotsBeforeRest = 2;
                shootBeat = global.wholeSec;
                shootTimer = shootBeat;
                gatlingShot = 1;
                script_execute(scr_fliptoplayer);
            }
            else if (gatlingShot == 1)
            {
                gatlingShot = 0;
                shootBeat = global.eighthSec;
                shootTimer = shootBeat;
                script_execute(scr_fliptoplayer);
                
            }
            
        }
    }
    
    //Grenades
    if (chosenAttack == 1)
    {
        //This code segment shoots a huge surrounding circle
        if (attackDelay &lt; 1)
        {
            attackDelay += 1;
            shootBeat = global.quarterSec;
            shootTimer = shootBeat;
        }
        else
        {
            if (shotCount &lt; 5 &amp;&amp; shotCount &gt; 0)
            {
                if (instance_exists(grenade[shotCount - 1]))
                {
                    if (grenade[shotCount - 1] != noone)
                    {
                        warnLaser = instance_create(x, y, obj_warninglaser_sonii);
                        warnLaser.direction = point_direction(x, y, grenade[shotCount - 1].x, grenade[shotCount - 1].y);
                        warnLaser.image_angle = point_direction(x, y, grenade[shotCount - 1].x, grenade[shotCount - 1].y);
                        audio_play_sound_on (global.effectsEmitter, snd_bombbeep, false, 10); 
                        shootBeat = global.eighthSec;
                        shootTimer = shootBeat;
                    }
                }
            }
            else if (shotCount &gt; 4 &amp;&amp; shotCount &lt; 9)
            {
                if (instance_exists(grenade[shotCount - 5]))
                {
                    if (grenade[shotCount - 5] != noone)
                    {
                        if (shotCount == 5)
                        {
                            audio_play_sound_on (global.voiceEmitter, snd_saulgrowl, false, 10); 
                        }
                    
                        sprite_index = spr_policechiefshoot;
                        realLaser = instance_create(x, y, obj_laser_sonii);
                        realLaser.direction = point_direction(x, y, grenade[shotCount - 5].x, grenade[shotCount - 5].y);
                        realLaser.image_angle = point_direction(x, y, grenade[shotCount - 5].x, grenade[shotCount - 5].y);
                        audio_play_sound_on (global.effectsEmitter, snd_waveblast, false, 10); 
                        shootBeat = global.quarterSec;
                        shootTimer = shootBeat;
                    }
                }
            }
            script_execute(scr_fliptoplayer);
            /*
            angleShot = -20;
            script_execute(scr_fliptoplayer);
            sprite_index = spr_lizshoot;
            image_index = 0;
            image_speed = 1;
            audio_play_sound_on (global.effectsEmitter, snd_bombthrow, false, 10); 
            for (i = 0; i &lt; 2; i++)
            {         
                var shot = instance_create(x, y, obj_grenadespread_sonii);
                if (instance_exists(shot))
                {
                    shot.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + random_range(-20, 20);
                    shot.speed = random_range(3, 5);
                    shot.shootBeat = global.quarterSec;
                    shot.shootTimer = shot.shootBeat;
                }
            }
            */
        }

    }
        
    //Micronesian Wave
    if (chosenAttack == 2)
    {
        if (shotCount == 1 || shotCount == 5 || shotCount == 9)
        {
            if (instance_exists(obj_chizuru))
            {
                move_towards_point(obj_chizuru.x, obj_chizuru.y, followspeed * 2);
                
            }
            shootBeat = global.quarterSec;
            shootTimer = shootBeat;
        }
        if (shotCount == 2 || shotCount == 6 || shotCount == 10)
        {
            laserAngle = random_range(0, 90);
            audio_play_sound_on (global.effectsEmitter, snd_bombbeep, false, 10); 
            audio_play_sound_on (global.voiceEmitter, snd_saulcharge, false, 10); 
            for (i = 0; i &lt; 360; i += 90)
            {
                warnBeam = instance_create(x, y, obj_warninglaser_sonii);
                warnBeam.direction = laserAngle + i;
                warnBeam.image_angle = laserAngle + i;
                warnBeam.damage = 12;
            }
            vspeed = 0;
            hspeed = 0;
            shootBeat = global.quarterSec;
            shootTimer = shootBeat;
        }
        if (shotCount == 3 || shotCount == 7 || shotCount == 11)
        {
            audio_play_sound_on (global.effectsEmitter, snd_crashed, false, 10); 
            bomb = instance_create(x, y, obj_lizexplosion);
            bomb.friendly = 0;
            bomb.image_angle = laserAngle;
            bomb.alarm[0] = 15;
            shootBeat = global.quarterSec;
            shootTimer = shootBeat;
        }
        if (shotCount == 4 || shotCount == 8)
        {
            audio_play_sound_on (global.effectsEmitter, snd_ricebullet, false, 10); 
            bomb = instance_create(x, y,  obj_grenadespread_sonii);
            bomb.speed = 7;
            if (instance_exists(obj_chizuru))
            {
                bomb.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + random_range(-80, 80);
            }
            shootBeat = global.quarterSec;
            shootTimer = shootBeat;
        }
    }
        
    
    
    // Beat indicator
    var beatIndicator = instance_create(x, y, obj_beatindicator_david);
    beatIndicator.image_speed = shootBeat * 40;
    
    
    
    if (chosenAttack == -1)
    {
        shootBeat = originalShootBeat;
        chosenAttack = 0;
    }
}

//Gatling Shot
if (gatlingShot == 1)
{
    audio_play_sound_on (global.effectsEmitter, snd_gatling, false, 10); 
    
    sprite_index = spr_policechiefshoot;
    image_index = 0;
    image_speed = 1;
    
    for (i = 0; i &lt; 3; i++)
    {
        topBarrier = instance_create(x, y, obj_rapidbullet);
        topBarrier.direction = aimCenter + 50 + random_range(-10, 10);
        topBarrier.speed = random_range(13, 15);
        topBarrier.damage = 12;
    }
    
    for (i = 0; i &lt; 3; i++)
    {
        bottomBarrier = instance_create(x, y, obj_rapidbullet);
        bottomBarrier.direction = aimCenter - 50 + random_range(-10, 10);
        bottomBarrier.speed = random_range(13, 15);
        bottomBarrier.damage = 12;
    }
    
    if (random_range(0, 100) &lt; 40)
    {
        for (i = 0; i &lt; 3; i++)
        {
            gatling = instance_create(x, y, obj_rapidbullet);
            gatling.direction = aimCenter + random_range(-40, 40);
            if (instance_exists(obj_chizuru))
            {
                gatling.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + random_range(-10, 10);
                
            }
            gatling.speed = 11 + (i * 0.5);
            gatling.damage = 12;
            
        }
    }
    
    aimDirection += 5;
    
    if (aimDirection &gt; 60)
    {
        aimDirection = -aimDirection;
    }
    gatlingShot = 2;
}
else if(gatlingShot == 2)
{
    gatlingShot = 1;
}



// ======================================================

if (moveTimer &lt;= 0)
{
    // ++++++++++++++ MOVING ++++++++++++++++++++++++++++
    //Always set the timer again so it loops
    moveTimer = moveBeat;
    
    //Action is checked on the step state to see if enemy is hurt or enemy is offscreen
    if(action == 1 &amp;&amp;  chosenAttack == 0)
    {
        //Touch these to modify movement
        followspeed = 10;
        minimumspeed = 5;
        maximumspeed = 8;
        
        //Do not touch these
        script_execute(scr_enemywallbeforemove);
        if (wallDetected == 0)
        {
        
            if (random_range(0, 100) &gt; 50 &amp;&amp; instance_exists(obj_chizuru))
            {
                move_towards_point(obj_chizuru.x, obj_chizuru.y, followspeed);
            }
            else
            {
                hspeed = random_range(minimumspeed, maximumspeed);
                vspeed = random_range(minimumspeed, maximumspeed);
                if (random_range(0, 100) &gt; 50)
                {
                    hspeed = -hspeed;
                }
                if (random_range(0, 100) &gt; 50)
                {
                    vspeed = -vspeed;
                }
            
            }
        
        
        }

    }
}

script_execute(scr_truespeedwallcheck);
// +++++++++++++++++++++++++++++++++++++++++++++++++++++++


/////////////////////////////
//Death
/////////////////////////
if (hp &lt; 1 &amp;&amp; global.bossone &lt; 1 &amp;&amp; global.bosstwo &lt; 1 &amp;&amp; global.bossthree &lt; 1)
{
    audio_stop_sound(snd_bosstheme);
    audio_play_sound_on (global.voiceEmitter, snd_lizdeath, false, 10); 
    var death = instance_create(x, y, obj_bossdeath);
    death.sprite_index = spr_lizhurt;
    
    if (!steam_get_achievement("BOSS3"))
    {
        steam_set_achievement("BOSS3");
    }
    
    if (global.highestLevel &lt; 3)
    {
        global.highestLevel = 3;
    }
    script_execute(scr_autosave);
    
    instance_destroy(self);
}
