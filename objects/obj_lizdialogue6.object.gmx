global.categoryArray[27] = "left";
global.emotionProtagonistArray[27] = "curious";
global.emotionAntagonistArray[27] = "sad";

global.messageArray[28] = "But what even happened? I think you were a great
senator that cared about everyone.";
global.categoryArray[28] = "right";
global.emotionProtagonistArray[28] = "curious";
global.emotionAntagonistArray[28] = "sad";


global.messageArray[29] = "I don't know, but a few of the new people 
