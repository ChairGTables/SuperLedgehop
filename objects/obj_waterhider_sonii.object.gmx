        <arguments>
          <argument>
            <kind>1</kind>
            <string>//////////////////////////
// Core-functionalities
// Do not touch these
// Scroll all the way down for changeable features
/////////////////////////



script_execute(scr_enemycorestep);



////////////////////////////
//Add additional step features here
////////////////////////////

//Spin animation for spinning enemies (comment this out if non-spinner)
//image_angle += 5;


shootTimer -= delta_time / 1000000;
moveTimer  -= delta_time / 1000000;

script_execute(scr_resyncenemies_david);

if (hittable == 0)
{
    image_alpha = 0.2;
}
else
{
    image_alpha = 1;
}


if (shootTimer &lt;= 0)
{
    
    // ======== SHOOTING ====================
    //Reset timer
    shootTimer = shootBeat;
    
    //Perform shooting
    if (action == 1)
    {
        //This code segment shoots a huge surrounding circle
        audio_stop_sound (snd_enemybullet);
        audio_play_sound_on (global.effectsEmitter, snd_enemybullet, false, 10); 
        if(beatCount == 0 )//|| beatCount == 1)
        {
            //underneath
            hittable = 0;
        }
        else if( beatCount == 1 || beatCount == 2 )
        {
            //reveal
            if(beatCount == 1)
            {
                //code to change
                hittable = 1;
                warnIndicator = instance_create (x,y, obj_circlewarn);
            }
            else
                hittable = 1;
                
                
            

            
        }
        else if( beatCount == 3)
        {
            //shooting
            audio_play_sound_on (global.effectsEmitter, snd_enemybullet, false, 10); 
            audio_play_sound_on (global.effectsEmitter, snd_icebullet, false, 10); 
            for (i = 0; i &lt; 2; i++)
            {
                
                shot = instance_create(x, y, obj_enemyicebullet);
                shot.creating = 1;
                shot.speed = 15;
                
                if (instance_exists(obj_chizuru))
                {
                    if (i == 0)
                    {
                        shot.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + 120;
                        shot.targetDirection = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) - 10;
                    }
                    else if (i == 1)
                    {
                        shot.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) - 120;
                        shot.targetDirection = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + 10;
                    }
                    
                }
                
                
            }
        }
        else if( beatCount &gt;= 8 &amp;&amp; beatCount &lt;= 12 )
        {
            randomAngle = random_range(-20, 20);
            audio_play_sound_on (global.effectsEmitter, snd_enemybullet, false, 10); 
            for (i = 0; i &lt; 2; i++)
            {
                shot = instance_create(x, y, obj_enemyicebullet);
                shot.speed = 13;
                
                shot.targetSpeed = 2;
                
                if (instance_exists(obj_chizuru))
                {
                    shot.direction = randomAngle + (-10 + (20 * i )) + point_direction(x, y, obj_chizuru.x, obj_chizuru.y);
                }
            }
        }
    
        
        
    }
    
    if( beatCount == 15 )
    {
        beatCount += 1
        hittable = 0;
    }
    else if (beatCount == 18)
    {
        beatCount = 0;
    }
    else
    {
        beatCount += 1;
    }
}
// ======================================================


if (moveTimer &lt;= 0)
{
    // ++++++++++++++ MOVING ++++++++++++++++++++++++++++
    //Always set the timer again so it loops
    moveTimer = moveBeat;
    
    //Action is checked on the step state to see if enemy is hurt or enemy is offscreen
    if(action == 1)
    {
        //Touch these to modify movement
        followspeed = 7;
        minimumspeed = 5;
        maximumspeed = 10;
        
        if (instance_exists(obj_chizuru))
        {
            move_towards_point(obj_chizuru.x, obj_chizuru.y, followspeed);
        }
        script_execute(scr_truespeedwallcheck);
    }
}
// +++++++++++++++++++++++++++++++++++++++++++++++++++++++


/////////////////////////////
//Death
/////////////////////////
if (hp &lt; 1)
{
    script_execute(scr_enemydeath);
}
