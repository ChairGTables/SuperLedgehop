            <kind>1</kind>
            <string>//Set health for enemy
hp = 20;

//set animation speed
image_speed = 0.5;

//You must define the hurt sprite as a string
hurtSprite = "spr_spreadmovehurt_sonii";

//Do not touch this
sS = sprite_index;
hS = asset_get_index(hurtSprite);

//Define the knockback and hitstun
bulletKnockback = 5;
shineKnockback = 7;
hitstunTime = 13;

//Define the beat that sets the alarm events to occur
shootBeat = global.halfSec;    // whole note
moveBeat = global.quarterSec; // half note

shootRestBeat = global.wholeSec * 10;

//Use these to shoot x amount of times before the enemy shoots again
shotCount = 0;
shotsBeforeRest = 3;
shootRestTime = 0;
action = 1;

//Do not touch these
upmomentum = 0;
rightmomentum = 0;
leftmomentum = 0;
downmomentum = 0;

//Begin the alarms based on the beats
//alarm[0] = shootBeat;
shootTimer = shootBeat;
//alarm[1] = moveBeat;
moveTimer = moveBeat;
</string>
