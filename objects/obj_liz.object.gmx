        <arguments>
          <argument>
            <kind>1</kind>
            <string>//////////////////////////
// Core-functionalities
// Do not touch these
// Scroll all the way down for changeable features
/////////////////////////

script_execute(scr_enemycorestep);

////////////////////////////
//Add additional step features here
////////////////////////////

shootTimer -= delta_time / 1000000;
moveTimer  -= delta_time / 1000000;

script_execute(scr_resyncenemies_david);

if (global.bosstwo &gt; 0 &amp;&amp; global.bossthree &lt; 1 &amp;&amp; global.bossone &gt; 0)
{
    if(hpIndex == 3)
    {
        hp = global.bossthree;
        hpIndex = 1;
    }
    global.bosstwo = hp;
}

if (global.bosstwo &lt; 1 &amp;&amp; global.bossthree &lt; 1 &amp;&amp; global.bossone &gt; 0)
{
    if(hpIndex == 2)
    {
        hp = global.bossone;
        hpIndex = 1;
    }
    global.bossone = hp;
}

if (shootTimer &lt;= 0)
{
    // ======== SHOOTING ====================
    //Reset timer
    if (shotCount &lt; shotsBeforeRest)
    {
        shootBeat = global.quarterSec;
        shootTimer = shootBeat;
        shotCount += 1;
    }
    else
    {
        shotCount = 0;
        chosenAttack = -1;
        attackDelay = 0;
        gatlingShot = 0;
        shootBeat = global.wholeSec;
        shootTimer = shootBeat;
    }
    
    //Perform shooting
    if (chosenAttack = 0)
    {
        
        
        //Must shoot 2 regulars first so cancel top if so
        if (normalAttack &lt; 2)
        {
            chosenAttack = 3;
            normalAttack += 1;
            audio_play_sound_on (global.voiceEmitter, snd_lizfreeze, false, 10); 
            sprite_index = spr_lizshoot;
            image_index = 0;
            image_speed = 1;
            script_execute(scr_fliptoplayer);
            
            if (instance_exists(obj_chizuru))
            {
                aimCenter = point_direction(x, y, obj_chizuru.x, obj_chizuru.y);
            }
            aimDirection = 0;
            
        }
        else
        {
            normalAttack = 0;
            audio_play_sound_on (global.effectsEmitter, snd_pineapplethrow, false, 10); 
            if (random_range(0, 100) &gt; 50)
            {
                var bomber = instance_create (x,y, obj_lizflashbang);
                audio_play_sound_on (global.voiceEmitter, snd_lizgiggle, false, 10); 
            }
            else
            {
                var bomber = instance_create (x,y, obj_lizpineapple);
                if (random_range(0, 100) &gt; 50)
                {
                    audio_play_sound_on (global.voiceEmitter, snd_lizvulcano, false, 10); 
                }
                else
                {
                    audio_play_sound_on (global.voiceEmitter, snd_lizburn, false, 10); 
                }
                
            }
            
            if (instance_exists(obj_chizuru))
            {
                bomber.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y);
            }
            
            bomber.speed = 10;
            
            if (60 &gt; 50)
            {
                chosenAttack = 1;
                shotsBeforeRest = 3;
            }
            else
            {
                //shotsBeforeRest = 4;
                //chosenAttack = 2;
                //audio_play_sound (snd_titowave, 10, false); 
            }
        }
    }
    
    //Gatling Attack
    if (chosenAttack == 3)
    {
        if(attackDelay &lt; 1)
        {
            attackDelay += 1;
        }
        else
        {
            if (gatlingShot == 0)
            {
                shotsBeforeRest = 2;
                shootBeat = global.wholeSec;
                shootTimer = shootBeat;
                gatlingShot = 1;
                script_execute(scr_fliptoplayer);
            }
            else if (gatlingShot == 1)
            {
                gatlingShot = 0;
                shootBeat = global.eighthSec;
                shootTimer = shootBeat;
                script_execute(scr_fliptoplayer);
                
            }
            
        }
    }
    
    //Red Rice Attack
    if (chosenAttack == 1)
    {
        //This code segment shoots a huge surrounding circle
        if (attackDelay &lt; 1)
        {
            attackDelay += 1;
            shootBeat = global.halfSec;
            shootTimer = shootBeat;
        }
        else
        {
            angleShot = -20;
            script_execute(scr_fliptoplayer);
            sprite_index = spr_lizshoot;
            image_index = 0;
            image_speed = 1;
            audio_play_sound_on (global.effectsEmitter, snd_bombthrow, false, 10); 
            for (i = 0; i &lt; 2; i++)
            {         
                var shot = instance_create(x, y, obj_grenadespread_sonii);
                if (instance_exists(shot))
                {
                    shot.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + random_range(-20, 20);
                    shot.speed = random_range(3, 5);
                    shot.shootBeat = global.wholeSec;
                    shot.shootTimer = shot.shootBeat;
                }
            }
        }

    }
        
    //Micronesian Wave
    if (chosenAttack == 2)
    {
        //This code segment shoots a huge surrounding circle
        if (attackDelay &lt; 1)
        {
            attackDelay += 1;
            shootBeat = global.halfSec
            shootTimer = shootBeat;
        }
        else
        {
            audio_play_sound (snd_waveblast, 10, false); 
            script_execute(scr_fliptoplayer);
            sprite_index = spr_lizshoot;
            image_index = 0;
            image_speed = 1;
            openArc = random_range(0, 50)
            for (i = 0; i &lt; 60; i++)
            {         
                if (i &lt; openArc || i &gt; openArc + 10)
                {
                    var shot = instance_create(x, y, obj_enemyenhancedbullet);
                    shot.direction = i * 6;
                    shot.speed = 7;
                    shot.sprite_index = spr_chizuruspreadbullet;
                    shot.image_xscale = 2;
                    shot.image_yscale = 2;
                    shot.category = "spread";
                }

                
            }
            
            if (instance_exists(obj_camera_chair))
            {
                obj_camera_chair.intensity = 20;
            }
        }

    }
        
    
    
    // Beat indicator
    var beatIndicator = instance_create(x, y, obj_beatindicator_david);
    beatIndicator.image_speed = shootBeat * 40;
    
    
    
    if (chosenAttack == -1)
    {
        shootBeat = originalShootBeat;
        chosenAttack = 0;
    }
}

//Gatling Shot
if (gatlingShot == 1)
{
    audio_play_sound_on (global.effectsEmitter, snd_gatling, false, 10); 
    
    sprite_index = spr_lizshoot;
    image_index = 0;
    image_speed = 1;
    var gatling = instance_create(x, y, obj_rapidbullet);
    if (instance_exists(gatling))
    {
        gatling.direction = aimCenter + aimDirection;
        gatling.speed = 9;
    }
    var gatling2 = instance_create(x, y, obj_rapidbullet);
    if (instance_exists(gatling2))
    {
        gatling2.direction = aimCenter - aimDirection;
        gatling2.speed = 9;
    }
    
    aimDirection += 5;
    
    if (aimDirection &gt; 60)
    {
        aimDirection = -aimDirection;
    }
    gatlingShot = 2;
}
else if(gatlingShot == 2)
{
    gatlingShot = 1;
}



// ======================================================

if (moveTimer &lt;= 0)
{
    // ++++++++++++++ MOVING ++++++++++++++++++++++++++++
    //Always set the timer again so it loops
    moveTimer = moveBeat;
    
    //Action is checked on the step state to see if enemy is hurt or enemy is offscreen
    if(action == 1 &amp;&amp;  chosenAttack == 0)
    {
        //Touch these to modify movement
        followspeed = 10;
        minimumspeed = 7;
        maximumspeed = 12;
        
        //Do not touch these
        script_execute(scr_enemywallbeforemove);
        if (wallDetected == 0)
        {
        
            if (random_range(0, 100) &gt; 50 &amp;&amp; instance_exists(obj_chizuru))
            {
                move_towards_point(obj_chizuru.x, obj_chizuru.y, followspeed);
            }
            else
            {
                hspeed = random_range(minimumspeed, maximumspeed);
                vspeed = random_range(minimumspeed, maximumspeed);
                if (random_range(0, 100) &gt; 50)
                {
                    hspeed = -hspeed;
                }
                if (random_range(0, 100) &gt; 50)
                {
                    vspeed = -vspeed;
                }
            
            }
        
        
        }

    }
}
// +++++++++++++++++++++++++++++++++++++++++++++++++++++++


/////////////////////////////
//Death
/////////////////////////
if (hp &lt; 1 &amp;&amp; global.bossone &lt; 1 &amp;&amp; global.bosstwo &lt; 1 &amp;&amp; global.bossthree &lt; 1)
{
    audio_stop_sound(snd_bosstheme);
    audio_play_sound_on (global.voiceEmitter, snd_lizdeath, false, 10); 
    var death = instance_create(x, y, obj_bossdeath);
    death.sprite_index = spr_lizhurt;
    instance_destroy(self);
}
</string>
