        <arguments>
          <argument>
            <kind>1</kind>
            <string>//////////////////////////
// Core-functionalities
// Do not touch these
// Scroll all the way down for changeable features
/////////////////////////

script_execute(scr_enemycorestep);

////////////////////////////
//Add additional step features here
////////////////////////////

shootTimer -= delta_time / 1000000;
moveTimer  -= delta_time / 1000000;

script_execute(scr_resyncenemies_david);

if (global.bosstwo &gt; 0 &amp;&amp; global.bossthree &lt; 1 &amp;&amp; global.bossone &gt; 0)
{
    if(hpIndex == 3)
    {
        hp = global.bossthree;
        hpIndex = 1;
    }
    global.bosstwo = hp;
}

if (global.bosstwo &lt; 1 &amp;&amp; global.bossthree &lt; 1 &amp;&amp; global.bossone &gt; 0)
{
    if(hpIndex == 2)
    {
        hp = global.bossone;
        hpIndex = 1;
    }
    global.bossone = hp;
}

if (shootTimer &lt;= 0)
{
    shootBeat = global.quarterSec;
    shootTimer = shootBeat;
    
    if (chosenAttack == 0)
    {
        if (normalAttack &lt; 3)
        {
            hspeed = 0;
            vspeed = 0;
            chosenAttack = 3;
            normalAttack += 1;
            if (random_range(0, 100) &gt; 50)
            {
                audio_play_sound_on (global.voiceEmitter, snd_yuutoyeah, false, 10); 
            }
            else
            {
                audio_play_sound_on (global.voiceEmitter, snd_yuutothere, false, 10); 
            }
            sprite_index = spr_yuutoshootd;
            image_index = 0;
            image_speed = 0.1;
            script_execute(scr_fliptoplayer);
            warnIndicator = instance_create (x,y, obj_warninglaser_sonii);
            if (instance_exists(obj_chizuru))
            {
                warnIndicator.image_angle = point_direction(x, y, obj_chizuru.x, obj_chizuru.y);
            }
            aimDirection = 0;
            shootBeat = global.eighthSec;
        }
        else
        {
            normalAttack = 0;
            if (random_range(0, 100) &gt; 50)
            {
                chosenAttack = 1;
                audio_play_sound_on (global.voiceEmitter, snd_yuutoastro, false, 10); 
            }
            else
            {
                chosenAttack = 2;
                audio_play_sound_on (global.voiceEmitter, snd_yuutodeviation, false, 10); 
            }
            
        }
    }
    else if (chosenAttack == 1)
    {
        if (attackDelay != 1)
        {
            attackDelay = 1;
        }
        else
        {
            attackDelay = 0;
            script_execute(scr_fliptoplayer);
            sprite_index = spr_yuutoshootd;
            image_index = 0;
            image_speed = 0.1;
            audio_play_sound_on (global.effectsEmitter, snd_bombthrow, false, 10); 
            myRock.bounce = 1;
            myRock.speed = 30;
            if (instance_exists(obj_chizuru))
            {
                myRock.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y);
            }        
            chosenAttack = 0;
        }
    }
    else if (chosenAttack == 2)
    {
        if (attackDelay != 1)
        {
            attackDelay = 1;
        }
        else
        {
            attackDelay = 0;
            script_execute(scr_fliptoplayer);
            sprite_index = spr_yuutoshootd;
            image_index = 0;
            image_speed = 0.1;
            audio_play_sound_on (global.effectsEmitter, snd_bombthrow, false, 10); 
            myRock.rotationSpeed = 3;
            myRock.speed = 15;
            if (instance_exists(obj_chizuru))
            {
                myRock.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y);
            }        
            chosenAttack = 0;
        }
    }
    else if (chosenAttack == 3)
    {
        shootBeat = global.wholeSec;
        shootTimer = shootBeat;
        audio_play_sound (snd_waveblast, 10, false); 
        shot = instance_create(x, y, obj_laser_sonii);
        if (instance_exists(obj_chizuru))
        {
            shot.image_angle = warnIndicator.image_angle;
        }
        shotCount = 1;
        chosenAttack = 0;
            
        
    }
}


// ======================================================

if (moveTimer &lt;= 0)
{
    // ++++++++++++++ MOVING ++++++++++++++++++++++++++++
    //Always set the timer again so it loops
    moveTimer = moveBeat;
    
    //Action is checked on the step state to see if enemy is hurt or enemy is offscreen
    if(action == 1 &amp;&amp;  chosenAttack == 0 &amp;&amp; !instance_exists(obj_warninglaser_sonii))
    {
        //Touch these to modify movement
        followspeed = 10;
        minimumspeed = 7;
        maximumspeed = 12;
        
        //Do not touch these
        script_execute(scr_enemywallbeforemove);
        if (wallDetected == 0)
        {
        
            if (random_range(0, 100) &gt; 50 &amp;&amp; instance_exists(obj_chizuru))
            {
                move_towards_point(obj_chizuru.x, obj_chizuru.y, followspeed);
            }
            else
            {
                hspeed = random_range(minimumspeed, maximumspeed);
                vspeed = random_range(minimumspeed, maximumspeed);
                if (random_range(0, 100) &gt; 50)
                {
                    hspeed = -hspeed;
                }
                if (random_range(0, 100) &gt; 50)
                {
                    vspeed = -vspeed;
                }
            
            }
        
        
        }

    }
}
// +++++++++++++++++++++++++++++++++++++++++++++++++++++++


/////////////////////////////
//Death
/////////////////////////
if (hp &lt; 1 &amp;&amp; global.bossone &lt; 1 &amp;&amp; global.bosstwo &lt; 1 &amp;&amp; global.bossthree &lt; 1)
{
    audio_stop_sound(snd_yuutotheme);
    //audio_play_sound_on (global.voiceEmitter, deathSound, false, 10); 
    var death = instance_create(x, y, obj_bossdeath);
    death.sprite_index = spr_yuutohurt;
    death.name = "yuuto";
    if (instance_exists(obj_yuutorock))
    {
        instance_destroy(obj_yuutorock);
    }
    instance_destroy(self);
}
