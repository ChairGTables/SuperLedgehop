shootRestTime -= delta_time/100000;
        

script_execute(scr_resyncenemies_david);

if (global.resyncing)
{
    shootRestTime = global.currentEighthSec / 4;
    shootRestTime = global.currentWholeSec * 10;
}

if (shootTimer &lt;= 0)
{
    //done only if shootresttime &lt;= 0
    
    if( shootRestTime &lt;= 0 )
    {
    // ======== SHOOTING ====================
    //Reset timer
    shootTimer = shootBeat;
    
    //Perform shooting
        if (action == 1 &amp;&amp; global.isCutscene == false &amp;&amp; !instance_exists(obj_moose))
        {
        //This code segment shoots a huge surrounding circle
            audio_stop_sound (snd_enemybullet);
            audio_play_sound_on (global.effectsEmitter, snd_enemybullet, false, 10); 
            var shot = instance_create(x, y, obj_flame_sonii);
            for (i = 0; i &lt; random_range(2, 6); i++)
            {
                shot.direction = 270 + random_range(-15, 15);
                shot.speed = random_range(7,20);
            }
            
            if (shotCount mod 15 == 0)
            {
                anglePerShot = 20;
                for (i = 0; i &lt; 1; i++)
                {
                    var shot = instance_create(x, y, obj_flame_sonii);
                    if (instance_exists(shot))
                    {
                        shot.direction = 270 + random_range(-15, 15);
                        shot.speed = 7;
                    }
