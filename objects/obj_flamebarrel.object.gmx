            <string>//////////////////////////
// Core-functionalities
// Do not touch these
// Scroll all the way down for changeable features
/////////////////////////



script_execute(scr_enemycorestep);



////////////////////////////
//Add additional step features here
////////////////////////////

//Spin animation for spinning enemies (comment this out if non-spinner)
//image_angle += 5;


shootTimer -= delta_time / 1000000;
moveTimer  -= delta_time / 1000000;
shootRestTime -= delta_time/100000;
        

script_execute(scr_resyncenemies_david);

if (global.resyncing)
{
    shootRestTime = global.currentWholeSec * 10;
}

if (shootTimer &lt;= 0)
{
    //done only if shootresttime &lt;= 0
    
    if( shootRestTime &lt;= 0 )
    {
        // ======== SHOOTING ====================
        //Reset timer
        shootTimer = shootBeat;
    }
    
}
// ======================================================

if (moveTimer &lt;= 0)
{
    // ++++++++++++++ MOVING ++++++++++++++++++++++++++++
    //Always set the timer again so it loops
    moveTimer = moveBeat;
}
// +++++++++++++++++++++++++++++++++++++++++++++++++++++++


/////////////////////////////
//Death
/////////////////////////
if (hp &lt; 1)
{
    script_execute(scr_enemydeath);
    
    for (i = 0; i &lt; random_range(10, 15); i++)
    {
        fire = instance_create(x + random_range(-100, 100), y + random_range(-100, 100), obj_chizuruflame);
    }
    
    for (i = 0; i &lt; 360; i+=8)
    {
        for (j = 0; j &lt; 5; j++)
        {
            spreadShot = instance_create(x, y, obj_chizuruspreadbullet);
            spreadShot.speed = 9 + (0.5 * j) + random_range(-1, 4);
            spreadShot.direction = i + random_range(-8, 8);
        }
    }
}
