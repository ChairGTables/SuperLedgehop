shootTimer -= delta_time / 1000000;
moveTimer  -= delta_time / 1000000;
shootRestTime -= delta_time/100000;
        

script_execute(scr_resyncenemies_david);

if (global.resyncing)
{
    shootRestTime = global.currentWholeSec * 4;
}

if (shotCount &gt; 99 &amp;&amp; shotCount &lt; 110)
{
    speed = 20;
    direction = point_direction(x, y, warn[shotCount - 100].x, warn[shotCount - 100].y);
}

if (shootTimer &lt;= 0)
{
    //done only if shootresttime &lt;= 0
    
    if( shootRestTime &lt;= 0 )
    {
    // ======== SHOOTING ====================
    //Reset timer
    shootTimer = shootBeat;
    
    //Perform shooting
        if (1 == 1)
        {
            
            if (phase &lt; 3)
            {
                if (shotCount == 0)
                {
                    sprite_index = spr_defaultshoot;
                    image_index = 0;
                    image_speed = 0;
                    audio_play_sound_on (global.effectsEmitter, snd_defaultdash, false, 10); 
                    if (random_range(0, 100) &gt; 50)
                    {
                        audio_play_sound_on (global.voiceEmitter, snd_defaultya2, false, 10); 
                    }
                    if (instance_exists(obj_chizuru))
                    {
                        direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y);
                        speed = 20;
                    }
                    shootBeat = global.quarterSec;
                    shootTimer = shootBeat;
                    shotCount += 1;
                    //This code segment shoots a huge surrounding circle
                    /*audio_stop_sound (snd_enemybullet);
                    audio_play_sound_on (global.effectsEmitter, snd_enemybullet, false, 10); 
                    var shot = instance_create(x, y, obj_flame_sonii);
                    for (i = 0; i &lt; random_range(2, 6); i++)
                    {
                        shot.direction = point_direction(x,y,obj_chizuru.x,obj_chizuru.y) + random_range(-30,30);
                        shot.speed = random_range(7,20);
                    }
                    
                    if (shotCount mod 15 == 0)
                    {
                        anglePerShot = 20;
                        for (i = 0; i &lt; 360 / anglePerShot; i++)
                        {
                            var shot = instance_create(x, y, obj_flame_sonii);
                            if (instance_exists(shot))
                            {
                                shot.direction = i * anglePerShot;
                                shot.speed = 7;
                            }
                        }
                    }*/
        
                    
                    
                    /*anglePerShot = 29;
                    for (i = 0; i &lt; 87 / anglePerShot; i++)
                    {
                        var shot = instance_create(x, y, obj_spread_sonii);
                        if (instance_exists(shot))
                        {
                           shot.direction = (i * anglePerShot)  + point_direction(x,y,obj_chizuru.x,obj_chizuru.y) - 29;
                           shot.speed = 3;
                        }
                    }*/
                }
                else if (shotCount == 1)
                {
                    image_speed = 0.7;
                    shootBeat = global.eighthSec;
                    shootTimer = shootBeat;
                    shotCount = 2;
                    speed = 0;
                    audio_stop_sound (snd_enemybullet);
                    audio_play_sound_on (global.effectsEmitter, snd_waveblast, false, 10); 
                    if (random_range(0, 100) &gt; 50)
                    {
                        audio_play_sound_on (global.voiceEmitter, snd_defaultfire, false, 10); 
                    }
                    for (i = 0; i &lt; random_range(20, 30); i++)
                    {
                        shot = instance_create(x, y, obj_flame_sonii);
                        if (instance_exists(obj_chizuru))
                        {
                            shot.direction = direction + random_range(-30, 30);
                        }
                        shot.speed = random_range(1, 15);
                        
                    }
                }
                else if (shotCount == 2)
                {   
                    shotCount = 0;
                    shootBeat = global.wholeSec;
                    shootTimer = shootBeat;
                    shootRestTime = shootRestBeat;
                    phase += 1;
                }
            }
            else
            {
                if (shotCount == 0)
                {
                    if (random_range(0, 100) &gt; 50)
                    {
                        shotCount = 99;    
                        audio_play_sound_on (global.voiceEmitter, snd_defaultfire, false, 10); 
                        audio_play_sound_on (global.effectsEmitter, snd_defaultwarn, false, 10); 
                        if (instance_exists(obj_chizuru))
                        {
                            warn[0] = instance_create(obj_chizuru.x-96, obj_chizuru.y-96, obj_warnbox);
                            warn[1] = instance_create(obj_chizuru.x+96, obj_chizuru.y-96, obj_warnbox);
                            warn[2] = instance_create(obj_chizuru.x-96, obj_chizuru.y+96, obj_warnbox);
                            warn[3] = instance_create(obj_chizuru.x+96, obj_chizuru.y+96, obj_warnbox);
                        }
                    }
                    else
                    {
                        shotCount = 200;
                        audio_play_sound_on (global.voiceEmitter, snd_defaultgetlost, false, 10); 
                        sprite_index = spr_defaultshoot;
                        image_index = 0;
                        image_speed = 0.4;
                        speed = 0;
                        detonation = instance_create(view_xview - 700, view_yview - 400, obj_defaultdetonator);
                    }
                    shootBeat = global.quarterSec;
                    shootTimer = shootBeat;
                }
                else if (shotCount == 99)
                {
                    shotCount = 100;
                    audio_play_sound_on (global.voiceEmitter, snd_defaultya2, false, 10); 
                    audio_play_sound_on (global.effectsEmitter, snd_defaultdash, false, 10); 
                    direction = point_direction(x, y, warn[0].x, warn[0].y);
                    speed = 20;
                    shootBeat = global.wholeSec * 2;
                    shootTimer = shootBeat;
                }
            }
        }
    
        
    
    }
    
}
// ======================================================

if (moveTimer &lt;= 0)
{
    // ++++++++++++++ MOVING ++++++++++++++++++++++++++++
    //Always set the timer again so it loops
    moveTimer = moveBeat;
    
    //Action is checked on the step state to see if enemy is hurt or enemy is offscreen
    if(action == 1 &amp;&amp; (shotCount == 200 || shotCount == 0))
    {
        //Touch these to modify movement
        followspeed = 7;
        minimumspeed = 6;
        maximumspeed = 9;
        
        //Do not touch these
        if (random_range(0, 100) &gt; 50 &amp;&amp; instance_exists(obj_chizuru))
        {
            move_towards_point(obj_chizuru.x, obj_chizuru.y, followspeed);
        }
        else
        {
            hspeed = random_range(minimumspeed, maximumspeed);
            vspeed = random_range(minimumspeed, maximumspeed);
            if (random_range(0, 100) &gt; 50)
            {
                hspeed = -hspeed;
            }
            if (random_range(0, 100) &gt; 50)
            {
                vspeed = -vspeed;
            }
        }
    }
}
