          <argument>
            <kind>1</kind>
            <string>//Shoot at small intervals until the rest time
if (shotCount &lt; shotsBeforeRest - 1)
{
    alarm[0] = shootBeat;
    shotCount += 1;
}
else
{
    //rest for a long time before shooting again
    alarm[0] = shootRestTime;
    shotCount = 0;
}

//Perform shooting
if (action == 1)
{
    //I have provided example bullet patterns; try them out

    //This code segment shoots a couple of bullets towards the player with a random variation of 30 degrees
    /*
    for (i = 0; i &lt; 3; i++)
    {
        var shot = instance_create(x, y, obj_enemyenhancedbullet);
        shot.speed = 8;
        shot.direction = point_direction(x, y, obj_chizuru.x, obj_chizuru.y) + random_range(-30, 30);
    }
    */
    
    
    //This code segment shoots a huge surrounding circle
    
    anglePerShot = 3;
    for (i = 0; i &lt; 360 / anglePerShot; i++)
    {
        var shot = instance_create(x, y, obj_enemyenhancedbullet);
        if (instance_exists(shot))
        {
            shot.direction = i * anglePerShot;
            shot.speed = 5;
        }
        
    }
    
}
