            <string>//////////////////////////
// Core-functionalities
// Do not touch these
// Scroll all the way down for changeable features
/////////////////////////



script_execute(scr_enemycorestep);



////////////////////////////
//Add additional step features here
////////////////////////////

//Spin animation for spinning enemies (comment this out if non-spinner)
//image_angle += 5;


shootTimer -= delta_time / 1000000;
moveTimer  -= delta_time / 1000000;
shootRestTime -= delta_time/100000;
        

script_execute(scr_resyncenemies_david);

if (global.resyncing)
{
    shootRestTime = global.currentEighthSec / 4;
    shootRestTime = global.currentWholeSec * 10;
}

if (shootTimer &lt;= 0)
{
    //done only if shootresttime &lt;= 0
    
    if( shootRestTime &lt;= 0 )
    {
    // ======== SHOOTING ====================
    //Reset timer
    shootTimer = shootBeat;
    
    //Perform shooting
        if (action == 1)
        {
        //This code segment shoots a huge surrounding circle
            audio_stop_sound (snd_enemybullet);
            audio_play_sound_on (global.effectsEmitter, snd_enemybullet, false, 10); 
            var shot = instance_create(x, y, obj_flame_sonii);
            for (i = 0; i &lt; random_range(4, 8); i++)
            {
                shot.direction = point_direction(x,y,obj_chizuru.x,obj_chizuru.y) + random_range(-30,30);
                shot.speed = random_range(10,20);
            }
            
            if (shotCount mod 15 == 0)
            {
                anglePerShot = 20;
                for (i = 0; i &lt; 360 / anglePerShot; i++)
                {
                    var shot = instance_create(x, y, obj_flame_sonii);
                    if (instance_exists(shot))
                    {
                        shot.direction = i * anglePerShot;
                        shot.speed = 7;
                    }
                }
            }

            
            shotCount += 1;
            /*anglePerShot = 29;
            for (i = 0; i &lt; 87 / anglePerShot; i++)
            {
                var shot = instance_create(x, y, obj_spread_sonii);
                if (instance_exists(shot))
                {
                   shot.direction = (i * anglePerShot)  + point_direction(x,y,obj_chizuru.x,obj_chizuru.y) - 29;
                   shot.speed = 3;
                }
            }*/
        }
    
        
    
        if( shotCount &gt;= shotsBeforeRest )
        {
            shootRestTime = shootRestBeat;
            shotCount = 0;
        }
    
    }
    
}
// ======================================================

if (moveTimer &lt;= 0)
{
    // ++++++++++++++ MOVING ++++++++++++++++++++++++++++
    //Always set the timer again so it loops
    moveTimer = moveBeat;
    
    //Action is checked on the step state to see if enemy is hurt or enemy is offscreen
    if(action == 1 &amp;&amp; shotCount == 0)
    {
        //Touch these to modify movement
        followspeed = 4;
        minimumspeed = 3;
        maximumspeed = 7;
        
        //Do not touch these
        if (random_range(0, 100) &gt; 50 &amp;&amp; instance_exists(obj_chizuru))
        {
            move_towards_point(obj_chizuru.x, obj_chizuru.y, followspeed);
        }
        else
        {
            hspeed = random_range(minimumspeed, maximumspeed);
            vspeed = random_range(minimumspeed, maximumspeed);
            if (random_range(0, 100) &gt; 50)
            {
                hspeed = -hspeed;
            }
            if (random_range(0, 100) &gt; 50)
            {
                vspeed = -vspeed;
            }
        }
        script_execute(scr_truespeedwallcheck);
    }
}
// +++++++++++++++++++++++++++++++++++++++++++++++++++++++


/////////////////////////////
